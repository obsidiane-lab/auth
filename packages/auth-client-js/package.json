{
    "name": "@obsidiane/auth-sdk",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "gen:types": "openapi-typescript ../../openapi.json -o src/types.gen.ts",
        "prepublishOnly": "npm run gen:types && npm run build"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "devDependencies": {
        "openapi-typescript": "^7.0.0",
        "typescript": "^5.6.0"
    }
}
